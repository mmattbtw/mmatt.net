generator client {
  provider = "prisma-client-js"
  previewFeatures = ["dataProxy"]
  engineType = "dataproxy"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model posts {
  id       String @id @default(cuid())
  category String
  imageUrl String
  markdown String
  slug     String @unique
  title    String

  CreatedAt DateTime @default(now())
  UpdatedAt DateTime @updatedAt
}

model projects {
  id       String @id @default(cuid())
  category String
  imageUrl String
  markdown String
  slug     String @unique
  title    String
  status   String

  CreatedAt DateTime @default(now())
  UpdatedAt DateTime @updatedAt
}